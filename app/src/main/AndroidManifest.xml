<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pro.network.trymeadmin">


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />
    <meta-data
        android:name="pro.farmerShepherd.diary.app.MyAppGlideModule"
        android:value="GlideModule" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:name="pro.network.trymeadmin.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/tryme_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/tryme_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="theme, allowBackup">


        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name="pro.network.trymeadmin.StartActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNowhite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pro.network.trymeadmin.NaviActivity"
            android:label="@string/app_name" />


        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBufZZ6hR1jYcnwqXg2egbpvCU8Vbd2UU0" />

        <activity
            android:name="pro.network.trymeadmin.product.MainActivityProduct"
            android:label="@string/app_name"
            android:theme="@style/AppThemeSe.NoActionBar">

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

        </activity>
        <activity
            android:name="pro.network.trymeadmin.banner.MainActivityBanner"
            android:label="@string/app_name"
            android:theme="@style/AppThemeSe.NoActionBar">

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

        </activity>
        <activity
            android:name="pro.network.trymeadmin.order.MainActivityOrder"
            android:label="@string/app_name"
            android:theme="@style/AppThemeSe.NoActionBar">

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

        </activity>

        <activity
            android:name="pro.network.trymeadmin.feedback.MainActivityFeedback"
            android:label="@string/app_name"
            android:theme="@style/AppThemeSe.NoActionBar"></activity>

        <activity
            android:name="pro.network.trymeadmin.product.ProductRegister"
            android:label="Stock Register"
            android:theme="@style/AppThemeWhite" />
        <activity
            android:name="pro.network.trymeadmin.banner.BannerRegister"
            android:label="Banner Upload"
            android:theme="@style/AppThemeWhite" />
        <activity
            android:name="pro.network.trymeadmin.product.ProductUpdate"
            android:label="Stock Update"
            android:theme="@style/AppThemeWhite" />

        <service
            android:name="pro.network.trymeadmin.app.FirebaseMessageReceiver"
            android:exported="false"
            android:stopWithTask="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="pro.network.trymeadmin.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>
        <activity android:name=".app.ActivityMediaOnline" />

    </application>

</manifest>